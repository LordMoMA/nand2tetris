/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=sel, out=o1);
    And(a=a, b=o1, out=oa);
    And(a=sel, b=b, out=ob);
    Or(a=oa, b=ob, out=out);
}